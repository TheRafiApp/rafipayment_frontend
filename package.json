{
  "name": "rafi-payment",
  "version": "1.0.9",
  "description": "Rent management at your fingertips",
  "author": "Nick Ford <nickforddesign@gmail.com>",
  "repository": "https://github.com/TheRafiApp/rafipayment_frontend",
  "license": "MIT",
  "scripts": {
    "version": "node build/version.js",
    "start": "node build/dev-server.js",
    "ui": "node build/dev-server-ui.js",
    "build": "node build/build.js",
    "cordova": "node build/build-cordova.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "export NODE_ENV=test && jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "ios": "npm run cordova && phonegap emulate ios",
    "browser": "npm run cordova && phonegap emulate browser",
    "serve": "npm run cordova && phonegap serve"
  },
  "dependencies": {
    "animejs": "^2.0.2",
    "cordova-ios": "^4.4.0",
    "exports-loader": "^0.6.4",
